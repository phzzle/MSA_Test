datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Tenant {
  address   String?
  createdAt DateTime  @default(now())
  email     String?
  endDate   DateTime?
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  name      String?
  phone     String?
  startDate DateTime?
  updatedAt DateTime  @updatedAt
}
